/***********************
  ROUND BUTTONS
************************/
.round {
  border-radius: 24px;
}
/***********************
  CUSTON BTN VALUES
************************/

.btn {
    padding: 14px 24px;
    border: 0 none;
    font-weight: 500;
    letter-spacing: 1px;
    text-transform: uppercase;
    font-size: 15px;
    text-shadow:none !important;
    box-shadow: none;
}
.btn:focus, .btn:active:focus, .btn.active:focus {
    outline: 0 none;
    background: #A31F34;
}

.btn-primary {
    background: #A31F34;
    color: #ffffff;
}
.btn-primary:hover, .btn-primary:focus, .btn-primary:active, .btn-primary.active, .open > .dropdown-toggle.btn-primary {
    background: #c72b43;
}
.btn-primary:active, .btn-primary.active {
    background: #8D1E2F;
    box-shadow: none;
}

#register #register:hover #register:focus a{
  text-decoration: none !important;
  color:white !important;
}
